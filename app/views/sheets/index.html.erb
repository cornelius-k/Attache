<p id="notice"><%= notice %></p>

<h1>Sheets</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Template</th>
      <th>Dossier</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sheets.each do |sheet| %>
      <tr>
        <td><%= sheet.title %></td>
        <td><%= sheet.template %></td>
        <td><%= sheet.dossier %></td>
        <td><%= link_to 'Show', dossier_sheet_path(@dossier, sheet) %></td>
        <td><%= link_to 'Edit', edit_dossier_sheet_path(@dossier, sheet) %></td>
        <td><%= link_to 'Destroy', [@dossier, sheet], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sheet', new_dossier_sheet_path(@dossier) %>
